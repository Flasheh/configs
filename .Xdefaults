##############################################
############### Urxvt config #################
##############################################

## General
URxvt.termname: rxvt-256color
URxvt.buffered: true
URxvt.background: #121212
URxvt.foreground: #999999
URxvt.cursorColor: #5E468C
URxvt.geometry: 80x22
URxvt.transparent: 0
URxvt*urgentOnBell: true

## URL stuff
URxvt.perl-ext-common:      default,matcher
URxvt.urlLauncher:   /usr/bin/chromium-dev
URxvt.matcher.button:: 1
#URxvt.underlineURLs: true
URxvt.matcher.rend.0: fg16 RS_Uline

## Font
URxvt.font:xft:Liberation Mono:medium:size=10:antialias=false
URxvt.boldFont:xft:Liberation Mono:medium:size=10:antialias=false

## Xft
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hinstyle: hintslight

## border & scrollbar
URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderless: false

## transparency
URxvt*inheritPixmap: false
URxvt*tintColor: #080808
URxvt*shading: 0

## Cursor
URxvt.cursorBlink: true
#URxvt.pointerBlank: true

#############################################
################## Testin ###################
#############################################

# fonts
#URxvt.font:xft:profont:medium:size=11
#URxvt.boldFont:xft:profont:medium:size=11

